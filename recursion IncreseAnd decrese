import java.util.*;
import java.lang.*;

public class Main{
    public static void main(String[] args) throws Exception{
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        
          printDecresing(n);

        
    }
    
  
    
    public static void printDecresing(int n){
        if(n==0){
            return;
        }
        
        System.out.println(n);
        printDecresing(n-1);
           System.out.println(n);
    }
    
    
}

